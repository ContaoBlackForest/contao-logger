{
    "name": "contaoblackforest/contao-logger",
    "description": "PSR-3 logger bridge for Contao Open Source CMS",
    "keywords": [
        "contao",
        "logger",
        "psr-3"
    ],
    "type": "contao-module",
    "license": "LGPL-3.0+",
    "abandoned": true,
    "authors": [
        {
          "name": "Sven Baumann",
          "email": "baumann.sv@gmail.com",
          "role": "Developer"
        },
        {
          "name": "Dominik Tomasi",
          "email": "dominik.tomasi@gmail.com",
          "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/ContaoBlackForest/contao-logger/issues",
        "source": "https://github.com/ContaoBlackForest/contao-logger"
    },
    "require": {
        "php": ">=5.3",
        "contao/core": ">=2.11.11,<4-dev",
        "contao-community-alliance/composer-plugin": "~2.0",
        "contao-community-alliance/dependency-container": "~1.0",
        "psr/log": "~1.0",
        "monolog/monolog": "~1.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.3",
        "phpunit/phpunit": "~3.7"
    },
    "autoload": {
        "psr-4": {
            "Bit3\\Contao\\Logger\\": "src/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "contao": "system/modules/logger"
            }
        },
        "branch-alias": {
            "dev-master": "2.0.x-dev",
            "dev-develop": "2.1.x-dev"
        }
    },
    "replace": {
        "bit3/contao-logger": "self.version"
    }
}
